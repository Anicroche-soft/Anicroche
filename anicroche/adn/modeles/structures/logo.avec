@style [

    #logo {
        --c-flux: var(--c-vif);
    }

    #logo > a {
        --d-ecart: 0.5rem;
    }

    #logo .logotype-anicroche {
        height: 1.5rem;

        --d-fonte: 1.5rem;
    }

    .petit #logo .logotype-anicroche,
    .petit.portrait #logo {
        display: none;
    }
]

<div #logo>
    ancre-boite /
        alt
            "Anicroche"
        icone-anicroche
        logotype-anicroche
