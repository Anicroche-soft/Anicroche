@style [
    #tete {
        grid-area: 1 / 1 / 2 / 2;

        padding: 1rem;

        --c-fond: var(--c-analogue);
        --c-flux: var(--c-vif);
    }

    #tete h1 {
        --d-fonte: 1.5rem;
    }

    .petit.portrait #tete {
        display: none;
    }

    .petit #tete h1 {
        display: none;
    }
]

<header #tete>
    lien-boite /
        icone-anicroche
        <h1>
            "Anicroche"
